 '' || password === '' ){\n                    setAlert({msg:'All fields are Necessary.',alertType:'danger'});\n                }\n                else{\n                    login({email,password})\n                }\n           }\n    } \n\n    let history = useHistory();\n    if(isAuthenticated){\n        history.push('/home');\n      }\n    \n    return (\n            <center className=\"outer\">\n            <div className=\"inner\">\n            <Tabs defaultActiveKey=\"login\" className=\"tab\" id=\"uncontrolled-tab-example\">\n                <Tab eventKey=\"login\" title=\"Login\">\n                     <AuthForm type=\"login\" onsubmit={onsubmit} />\n                </Tab>\n                <Tab eventKey=\"register\" title=\"Register\">\n                     <AuthForm type=\"register\" onsubmit={onsubmit} />\n                </Tab>\n            </Tabs>\n            </div>\n            </center>\n    );\n}\n\nconst mapStateToProps = (state) => {\n    return {\n        isAuthenticated:state.auth.isAuthenticated\n    }\n}\n\nexport default connect(mapStateToProps,{register,login,setAlert})(Auth);","/home/adarsh/MERN Projects/Social Media Project/frontend/src/App.js",["167","168"],"/home/adarsh/MERN Projects/Social Media Project/frontend/src/components/Alert/Alert.js",[],["169","170"],"/home/adarsh/MERN Projects/Social Media Project/frontend/src/components/Auth/AuthForm.js",["171","172"],"/home/adarsh/MERN Projects/Social Media Project/frontend/src/index.js",[],["173","174"],"/home/adarsh/MERN Projects/Social Media Project/frontend/src/components/NavigationBar.js",["175"],"/home/adarsh/MERN Projects/Social Media Project/frontend/src/reducers/Auth.js",["176"],"const initialState = {\n    token:localStorage.getItem('token'),\n    isAuthenticated : false,\n    user : null\n};\n\nexport default (state = initialState,action) => {\n       const {type,payload} = action;\n       switch(type){\n             case 'Auth_Success':\n                 return {\n               